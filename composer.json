{
	"name": "timmyrs/phpcraft",
	"description": "A PHP library for all things Minecraft: Java Edition.",
	"keywords": [
		"minecraft-api",
		"minecraft-library",
		"nbt-library",
		"minecraft-protocol",
		"mcje",
		"minecraft-java",
		"minecraft"
	],
	"homepage": "https://github.com/timmyrs/Phpcraft",
	"type": "library",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Phpcraft\\": "src/"
		}
	},
	"require": {
		"php": "^7.0",
		"ext-SPL": "*",
		"ext-json": "^1.4",
		"ext-zlib": "*",
		"ext-mbstring": "*",
		"ext-gmp": "*",
		"ext-openssl": "*",
		"hell-sh/enum": "^1.0",
		"hell-sh/uuid": "^1.0",
		"hell-sh/pai": "^2.0",
		"phpseclib/mcrypt_compat": "^1.0"
	},
	"require-dev": {
		"hell-sh/nose": "^1.1"
	},
	"suggest": {
		"ext-mcrypt": "Install the mcrypt extension to speed up \"online mode\" connections."
	}
}
