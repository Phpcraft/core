{
	"name": "timmyrs/phpcraft",
	"description": "A PHP library for all things Minecraft.",
	"keywords": ["minecraft", "minecraft-protocol"],
	"homepage": "https://github.com/timmyrs/Phpcraft",
	"type": "library",
	"license": "MIT",
	"require": {
		"php-64bit": "^7.0.15",
		"ext-mbstring": "*"
	},
	"require-dev": {
		"ext-gmp": "*",
		"ext-openssl": "*",
		"ext-mcrypt": "*"
	},
	"autoload": {
		"files": [
			"src/Account.class.php",
			"src/Connection.class.php",
			"src/ClientConnection.class.php",
			"src/Exception.class.php",
			"src/Packet.class.php",
			"src/KeepAlivePacket.class.php",
			"src/KeepAliveRequestPacket.class.php",
			"src/KeepAliveResponsePacket.class.php",
			"src/Phpcraft.class.php",
			"src/PlainUserInterface.class.php",
			"src/Server.class.php",
			"src/ServerConnection.class.php",
			"src/UserInterface.class.php"
		]
	},
	"optimize-autoloader": true
}